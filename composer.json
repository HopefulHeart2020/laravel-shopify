{
  "name": "ohmybrew/shopify-app",
  "description": "Shopify App helper for Laravel",
  "keywords": ["laravel", "shopify"],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Tyler King",
      "email": "tyler.n.king@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "5.4.*",
    "ohmybrew/basic-shopify-api": "1.0.*",
    "doctrine/dbal": "^2.5"
  },
  "require-dev":{
      "orchestra/database": "~3.1",
      "orchestra/testbench": "~3.0",
      "phpunit/phpunit": "~5.7",
      "satooshi/php-coveralls": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "OhMyBrew\\ShopifyApp\\": "src/ShopifyApp"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OhMyBrew\\ShopifyApp\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OhMyBrew\\ShopifyApp\\ShopifyAppProvider"
      ]
    }
  },
  "config": { "bin-dir": "bin" }
}
